<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Instructor" table="instructor">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstName" type="string" column="first_name" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastName" type="string" column="last_name" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rank" type="string" column="rank" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="qualificationId" type="integer" column="qualification_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="battalion" type="string" column="battalion" length="45" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="detachment" type="string" column="detachment" length="45" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="company" type="string" column="company" length="45" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
