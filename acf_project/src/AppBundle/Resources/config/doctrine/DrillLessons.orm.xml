<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\DrillLessons" table="drill_lessons">
    <indexes>
      <index name="drill_lessons_syllabus_id_fk" columns="syllabus_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="drill_lessons_id_uindex" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="125" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="chapter" type="string" column="chapter" length="25" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="section" type="string" column="section" length="25" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="noOfPeriods" type="integer" column="no_of_periods" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="subTitle" type="string" column="sub_title" length="125" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <many-to-one field="syllabus" target-entity="Syllabus" fetch="LAZY">
      <join-columns>
        <join-column name="syllabus_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
