<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Syllabus" table="syllabus">
    <unique-constraints>
      <unique-constraint name="syllabus_id_uindex" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="25" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="drillPeriods" type="integer" column="drill_periods" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="mkPeriods" type="integer" column="mk_periods" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="saaPeriods" type="integer" column="saa_periods" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="navPeriods" type="integer" column="nav_periods" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fieldcraft" type="integer" column="fieldcraft" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="faPeriods" type="integer" column="fa_periods" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="expedPeriods" type="integer" column="exped_periods" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="ptPeriods" type="integer" column="pt_periods" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="adminPeriods" type="integer" column="admin_periods" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="totalPeriods" type="integer" column="total_periods" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="shootingPeriods" type="integer" column="shooting_periods" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="cadetComPeriods" type="integer" column="cadet_com_periods" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="revisionPeriods" type="integer" column="revision_periods" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
