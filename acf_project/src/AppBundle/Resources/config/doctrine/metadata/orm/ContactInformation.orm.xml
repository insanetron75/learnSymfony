<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ContactInformation" table="contact_information">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rank" type="string" column="rank" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstName" type="string" column="first_name" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="secondName" type="string" column="second_name" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="affiliation" type="string" column="affiliation" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address1" type="string" column="address_1" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address2" type="string" column="address_2" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address3" type="string" column="address_3" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="city" type="string" column="city" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="region" type="string" column="region" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="string" column="country" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="postcode" type="string" column="postcode" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="primaryPhone" type="string" column="primary_phone" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="secondaryPhone" type="string" column="secondary_phone" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
